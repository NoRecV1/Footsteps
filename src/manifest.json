{
  "name": "Footsteps",
  "version": "0.0.1",
  "description": "What trails are you leaving behind?",
  "manifest_version": 2,
  "icons": {
    "512": "./assets/icons/logo-512x512.png",
    "192": "./assets/icons/logo-192x192.png",
    "32": "./assets/icons/favicon-32x32.png",
    "16": "./assets/icons/favicon-16x16.png"
  },
  "permissions": [
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}
