{
  "name": "Footsteps",
  "version": "0.0.1",
  "description": "Quelles traces laissez-vous sur le net?",
  "manifest_version": 2,
  "icons": {
    "512": "./assets/icons/logo-512x512.png",
    "192": "./assets/icons/logo-192x192.png",
    "32": "./assets/icons/favicon-32x32.png",
    "16": "./assets/icons/favicon-16x16.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}
